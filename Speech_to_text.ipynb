    "import speech_recognition as sr\n",
    "r=sr.Recognizer()\n",
    "with sr.Microphone() as source:\n",
    "    print(\"Speak Please:\")\n",
    "    audio=r.listen(source)\n",
    "try:\n",
    "    text=r.recognize_google(audio,language='hi-IN')\n",
    "    print(\"You said:  \"+text)\n",
    "except :\n",
    "    print(\"Please Run again\")"
   ]
